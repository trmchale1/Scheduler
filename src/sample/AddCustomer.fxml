<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>

<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml" prefHeight="400" prefWidth="400">
    <children>
        <Label layoutX="16.0" layoutY="14.0" style="-fx-label-padding: 0;" text="Add Customer">
            <font>
                <Font name="System Bold" size="28.0"/>
            </font>
        </Label>
      <TextField fx:id="id" editable="false" layoutX="188.0" layoutY="75.0"
                   prefHeight="40.0" prefWidth="150.0" style="-fx-background-color: cdcdcd;"/>
        <TextField fx:id="name" layoutX="188.0" layoutY="125.0"  prefHeight="40.0"
                   prefWidth="150.0"/>
        <TextField fx:id="address" layoutX="188.0" layoutY="175.0"  prefHeight="40.0"
                   prefWidth="150.0"/>
        <TextField fx:id="postal" layoutX="188.0" layoutY="225.0"  prefHeight="40.0"
                   prefWidth="150.0"/>
        <TextField fx:id="phone" layoutX="188.0" layoutY="275.0" prefHeight="40.0"
                   prefWidth="75.0"/>
        <Label layoutX="63.0" layoutY="75.0" text="Customer ID">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <Label layoutX="63.0" layoutY="125.0" text="Customer Name">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <Label layoutX="63.0" layoutY="175.0" text="Address">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <Label layoutX="63.0" layoutY="225.0" text="Postal Code">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <Label layoutX="63.0" layoutY="275.0" text="Phone">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <Label layoutX="63.0" layoutY="325.0" text="Division">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <Label layoutX="63.0" layoutY="375.0" text="Country">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <ChoiceBox fx:id="choice_div"  layoutX="188.0" layoutY="325.0" prefHeight="25.0" prefWidth="191.0"/>
        <ChoiceBox fx:id="choice_count"  layoutX="188.0" layoutY="375.0" prefHeight="25.0" prefWidth="191.0"/>
        <Button fx:id="cancelAddCustomer" layoutX="394.0" layoutY="485.0" mnemonicParsing="false" onMouseClicked="#cancelAddCustomer"
                 prefHeight="36.0" prefWidth="67.0" text="Cancel"/>
        <Button fx:id="saveButton" layoutX="183.0" layoutY="445.0" mnemonicParsing="false"
                prefHeight="40.0" prefWidth="110.0" text="Save">
            <font>
                <Font size="16.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>